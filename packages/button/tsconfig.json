{
  "module": "es6",
  "target": "es5",
  "lib": ["es6", "dom"],
  "sourceMap": true,
  "allowJs": false,
  "jsx": "react",
  "moduleResolution": "nodenext",
  "esModuleInterop": true,
  "allowSyntheticDefaultImports": true,
  "strict": true,
  "declaration": true,
  "exclude": ["node_modules"],
  "compilerOptions": {
    "jsx": "react"
  }
}
